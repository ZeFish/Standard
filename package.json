{
  "name": "@zefish/standard",
  "version": "0.10.18",
  "description": "A fine-art typography framework implementing classical design principles, mathematical precision with the golden ratio, and micro-typography rules inspired by the masters of print design.",
  "author": "Francis Fontaine",
  "license": "MIT",
  "homepage": "https://github.com/ZeFish/Standard#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeFish/Standard.git"
  },
  "bugs": {
    "url": "https://github.com/ZeFish/Standard/issues"
  },
  "keywords": [
    "css",
    "framework",
    "sass",
    "typography",
    "fine-art",
    "golden-ratio",
    "vertical-rhythm",
    "print-design",
    "micro-typography",
    "swiss-style",
    "baseline-grid",
    "classical-typography",
    "standard",
    "book-layout"
  ],
  "main": "src/eleventy.js",
  "exports": {
    "./css": "./dist/standard.min.css",
    "./js": "./dist/standard.min.js",
    "./system": "./dist/standard.system.js",
    ".": "./src/eleventy.js"
  },
  "files": [
    "dist",
    "src/eleventy.js"
  ],
  "scripts": {
    "dev": "concurrently \"npm:watch:*\"",
    "watch:css": "sass --watch --load-path=node_modules src/standard.scss dist/standard.css",
    "watch:js": "nodemon --watch src/standard.js --exec \"npm run build:js\"",
    "build": "npm run build:css && npm run build:css-min && npm run build:js && npm run build:js-min",
    "build:css": "sass --load-path=node_modules src/standard.scss dist/standard.css",
    "build:css-min": "sass --load-path=node_modules src/standard.scss dist/standard.min.css --style=compressed",
    "build:js": "cp src/standard.js dist/standard.js",
    "build:js-min": "terser src/standard.js -o dist/standard.min.js --compress --mangle && terser src/standard.system.js -o dist/standard.system.js",
    "clean": "rm -rf dist",
    "release": "npm run clean && npm run build && release-it --no-git.requireCleanWorkingDir --no-npm.requireCleanWorkingDir",
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "auto-changelog -p --template keepachangelog --output CHANGELOG.md",
    "eleventy": "eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "auto-changelog": "^2.5.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "release-it": "^19.0.5",
    "sanitize.css": "^13.0.0",
    "sass": "^1.93.2",
    "terser": "^5.44.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "chore: release v${version}",
      "requireCleanWorkingDir": false,
      "requireUpstream": false
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true,
      "web": false
    },
    "hooks": {
      "after:bump": "npm run changelog"
    },
    "prompt": {
      "src": false,
      "changelog": false,
      "commit": false,
      "tag": false,
      "push": false,
      "release": false,
      "publish": false
    }
  },
